<table>
  <tr>
    <th>Military history</th>
    <th>Awards</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @received_military_awards.each do |received_military_award| %>
  <tr>
    <td><%= received_military_award.military_history_id %></td>
    <td><%= received_military_award.awards %></td>
    <td><%= link_to 'Show', received_military_award %></td>
    <td><%= link_to 'Edit', edit_received_military_award_path(received_military_award) %></td>
