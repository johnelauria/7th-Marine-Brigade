<h1>Listing rids</h1>

<table>
  <tr>
    <th>Personal detail</th>
    <th>Province</th>
    <th>Province contact</th>
    <th>Religion</th>
    <th>Blood type</th>
    <th>Sss</th>
    <th>Tin</th>
    <th>Gsis</th>
    <th>Philhealth</th>
    <th>Person to notify</th>
    <th>Size of combat</th>
    <th>Size of cap</th>
    <th>Size of bdu</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rids.each do |rid| %>
  <tr>
    <td><%= rid.personal_detail_id %></td>
    <td><%= rid.province %></td>
    <td><%= rid.province_contact %></td>
    <td><%= rid.religion %></td>
    <td><%= rid.blood_type %></td>
    <td><%= rid.sss %></td>
    <td><%= rid.tin %></td>
    <td><%= rid.gsis %></td>
    <td><%= rid.philhealth %></td>
    <td><%= rid.person_to_notify %></td>
    <td><%= rid.size_of_combat %></td>
    <td><%= rid.size_of_cap %></td>
    <td><%= rid.size_of_bdu %></td>
    <td><%= link_to 'Show', rid %></td>
    <td><%= link_to 'Edit', edit_rid_path(rid) %></td>
    <td><%= link_to 'Destroy', rid, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Rid', new_rid_path %>
