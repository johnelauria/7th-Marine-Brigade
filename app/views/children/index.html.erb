<h1>Listing children</h1>

<table>
  <tr>
    <th>Marital histories</th>
    <th>Name</th>
    <th>Birth date</th>
    <th>Address</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @children.each do |child| %>
  <tr>
    <td><%= child.marital_histories_id %></td>
    <td><%= child.name %></td>
    <td><%= child.birth_date %></td>
    <td><%= child.address %></td>
    <td><%= link_to 'Show', child %></td>
    <td><%= link_to 'Edit', edit_child_path(child) %></td>
    <td><%= link_to 'Destroy', child, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Child', new_child_path %>
