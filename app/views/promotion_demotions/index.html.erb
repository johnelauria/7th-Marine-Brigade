<h1>Listing promotion_demotions</h1>

<table>
  <tr>
    <th>Rid</th>
    <th>From</th>
    <th>To</th>
    <th>Rank</th>
    <th>Authority</th>
    <th>Effectivity</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @promotion_demotions.each do |promotion_demotion| %>
  <tr>
    <td><%= promotion_demotion.rid_id %></td>
    <td><%= promotion_demotion.from %></td>
    <td><%= promotion_demotion.to %></td>
    <td><%= promotion_demotion.rank %></td>
    <td><%= promotion_demotion.authority %></td>
    <td><%= promotion_demotion.effectivity %></td>
    <td><%= link_to 'Show', promotion_demotion %></td>
    <td><%= link_to 'Edit', edit_promotion_demotion_path(promotion_demotion) %></td>
    <td><%= link_to 'Destroy', promotion_demotion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

